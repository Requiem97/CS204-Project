<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>204Project</display-name>
  <welcome-file-list>
   
    <welcome-file>index.jsp</welcome-file>
   
  </welcome-file-list>
  <context-param>
    <param-name>jndiLookupKey</param-name>
    <param-value>java:/comp/env/jdbc/ICS114-3CSA-DS</param-value>
  </context-param>
  <servlet>
    <servlet-name>Servlet1</servlet-name>
    <servlet-class>controller.Servlet1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>logic</servlet-name>
    <servlet-class>controller.logic</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>logic2</servlet-name>
    <servlet-class>controller.logic2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servlet1</servlet-name>
    <url-pattern>/Servlet1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>logic</servlet-name>
    <url-pattern>/logic</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>logic2</servlet-name>
    <url-pattern>/logic2</url-pattern>
  </servlet-mapping>
</web-app>